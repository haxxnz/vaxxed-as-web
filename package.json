  "name": "vaxxed-as-web",
  "version": "1.0.0",
  "private": true,
  "description": "Vaxxed As",
  "author": "Rafael Contreras",
  "keywords": [
    "gatsby"
  ],
  "scripts": {
    "build": "yarn gatsby:clean && gatsby:build",
    "commit": "gacp",
    "create-languages": "tsm ./scripts/createLanguages.ts",
    "get-did": "tsm ./scripts/did.ts",
    "gatsby:build": "gatsby build --no-color",
    "gatsby:clean": "gatsby clean",
    "gatsby:develop": "gatsby develop -H 0.0.0.0 -p 8000 -o",
    "gatsby:serve": "gatsby serve -H 0.0.0.0 -p 8001",
    "format": "prettier --write \"./**/*.{js,ts,css,json,md,mdx,html,yml,yaml,pcss,jsx,tsx}\"",
    "format:locales": "prettier --write \"./locales/**/*.{js,ts,css,json,md,mdx,html,yml,yaml,pcss,jsx,tsx}\"",
    "postinstall": "patch-package && yarn gatsby:clean",
    "lint": "eslint . --ext .tsx,.ts,.jsx,.js,.css,.scss",
    "lint:fix": "yarn lint --fix",
    "lint:package": "sort-package-json",
    "localazy:download": "export $(xargs <.env.development) && localazy download -r $LOCALAZY_READ_KEY && yarn format:locales",
    "localazy:keys": "export $(xargs <.env.development)",
    "localazy:upload": "export $(xargs <.env.development) && localazy upload -r $LOCALAZY_READ_KEY -w $LOCALAZY_WRITE_KEY",
    "postbuild:clear": "rm -rf -d ./public/**/*.map && rm -rf -d ./public/*.LICENSE.txt",
    "postbuild:sourcemaps": "node ./scripts/removeSourceMaps.mjs",
    "prepare": "husky install",
    "release": "standard-version",
    "release:major": "standard-version --release-as major  --no-verify",
    "release:minor": "standard-version --release-as minor  --no-verify",
    "release:patch": "standard-version --release-as patch  --no-verify",
    "security:fix": "npx yarn-audit-fix --audit-level=high",
    "remotion:develop": "remotion preview src/video/index.tsx",
    "remotion:build": "zx ./scripts/createOG.mjs"
  },
